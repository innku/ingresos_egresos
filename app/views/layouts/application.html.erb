<!DOCTYPE html>
<html>
<head>
  <title><%= yield :title %></title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="navbar">
    <div class="navbar-inner">
      <%= link_to 'Innku', root_path, class: 'brand' %>
      <ul class="nav">
        <li><%= link_to 'Home', root_path%></li>
        <li><%= link_to 'Proveedores', new_supplier_path %></li>
      </ul>
    </div>
  </div>
  
  <div class='container'>
    <div>
      <%= flash[:notice] %>
    </div>
    <%= yield %>
  </div>

</body>
</html>
